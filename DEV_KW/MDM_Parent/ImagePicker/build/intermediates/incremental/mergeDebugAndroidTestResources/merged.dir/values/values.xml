<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#000000</color>
    <color name="status_bar">#303135</color>
    <color name="theme_body">#cc22292c</color>
    <color name="top_bar">#9f556061</color>
    <color name="transparent">#00000000</color>
    <declare-styleable name="CropImageView"><attr format="color" name="cropMaskColor"/><attr format="color" name="cropBorderColor"/><attr format="dimension" name="cropBorderWidth"/><attr format="dimension" name="cropFocusWidth"/><attr format="dimension" name="cropFocusHeight"/><attr format="enum" name="cropStyle">
            <enum name="rectangle" value="0"/>
            <enum name="circle" value="1"/>
        </attr></declare-styleable>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <string name="all_images">全部图片</string>
    <string name="app_name">PhotoView Sample</string>
    <string name="complete">完成</string>
    <string name="folder_image_count">共%1$d张</string>
    <string name="menu_scale_center">Change to CENTER</string>
    <string name="menu_scale_center_crop">Change to CENTER_CROP</string>
    <string name="menu_scale_center_inside">Change to CENTER_INSIDE</string>
    <string name="menu_scale_fit_center">Change to FIT_CENTER</string>
    <string name="menu_scale_fit_end">Change to FIT_END</string>
    <string name="menu_scale_fit_start">Change to FIT_START</string>
    <string name="menu_scale_fit_xy">Change to FIT_XY</string>
    <string name="menu_zoom_disable">Disable Zoom</string>
    <string name="menu_zoom_enable">Enable Zoom</string>
    <string name="origin">原图</string>
    <string name="origin_size">原图(%1$s)</string>
    <string name="photo_crop">图片裁剪</string>
    <string name="preview_count">预览(%1$s)</string>
    <string name="preview_image_count">%1$s/%2$s</string>
    <string name="select_complete">完成(%1$s/%2$s)</string>
    <string name="select_limit">最多选择%1$s张图片</string>
    <style name="ImagePickerTheme" parent="AppBaseTheme">
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="ImagePickerThemeFullScreen" parent="ImagePickerTheme">
        <item name="android:fitsSystemWindows">false</item>
    </style>
    <style name="SuperCheckboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/selector_item_checked</item>
    </style>
    <style name="popupwindow_anim_style">
        <!-- 指定显示的动画xml -->
        <item name="android:windowEnterAnimation">@anim/show_from_bottom</item>
        <!-- 指定消失的动画xml -->
        <item name="android:windowExitAnimation">@anim/hide_to_bottom</item>
    </style>
</resources>