<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context="cn.kiway.mdm.activity.HomeActivity">

    <include
        android:id="@+id/titleRL"
        layout="@layout/layout_title"></include>


    <GridView
        android:id="@+id/studentGV"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/titleRL"
        android:layout_marginLeft="100dp"
        android:layout_marginTop="15dp"
        android:horizontalSpacing="10dp"
        android:numColumns="8"
        android:verticalSpacing="15dp"></GridView>


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:onClick="dianming"
        android:text="点名" />

</RelativeLayout>
